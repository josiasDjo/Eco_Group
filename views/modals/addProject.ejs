<div id="addProjectId" class="w-96 h-auto px-10 py-5 bg-white items-center justify-center hidden flex-col shadow-lg z-50 rounded-lg shadow-lg relative">
    <button type="button" class="absolute top-3 left-4 text-2xl txt_color_orange cursor-pointer" onclick="openAddProject('close');"><i class="fa-solid fa-rectangle-xmark" id="closeConnexion"></i></button>
    <h4 class="text-xl font-medium my-3">Ajouter un projet ou une activit√©</h4>
    <form id="addaProject" class="w-full flex flex-col" enctype="multipart/form-data">
        <input type="text" name="title" id="title_project" placeholder="Titre : " required class="w-full rounded-xl border-2 border-solid border-gray-200 px-3 py-2 my-2">
        <textarea name="description" id="description_project" rows="5" cols="50" required placeholder="Description du projet : " class="w-full rounded-xl border-2 border-solid border-gray-200 px-3 py-2 my-2"></textarea>
        <input type="file" name="image" id="image" required class="w-full rounded-xl border-2 border-solid border-gray-200 px-3 py-2 my-2" accept="image/*">
        <ul class="w-full text-center">
            <p id="msg_result" class="text-md"></p>
        </ul>
        <%- include('loader') %>
        <button type="submit" class="btn_bg_color w-full text-white rounded-xl border-2 border-solid border-yellow-500 px-3 py-2">Ajouter</button>
    </form>
</div>