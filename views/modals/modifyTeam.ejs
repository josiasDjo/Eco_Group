    <form id="ModifyToTeam" class="w-full flex flex-col" enctype="multipart/form-data">
        <input type="text" name="nom" id="nom_modify" placeholder="Nom : " required class="w-full rounded-xl border-2 border-solid border-gray-200 px-3 py-2 my-2">
        <input type="text" name="prenom" id="prenom_modify" placeholder="PrÃ©nom : " required class="w-full rounded-xl border-2 border-solid border-gray-200 px-3 py-2 my-2">
